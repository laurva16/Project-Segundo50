{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "co.edu.uptc.App",
            "projectName": "multimedia_project2"
        },
        {
            "type": "java",
            "name": "AppRun",
            "request": "launch",
            "mainClass": "co.edu.uptc.run.AppRun",
            "projectName": "multimedia_project2"
        },
        {
            "type": "java",
            "name": "pruebafx",
            "request": "launch",
            "mainClass": "co.edu.uptc.run.pruebafx",
            "projectName": "multimedia_project2",
            "vmArgs": "--module-path 'C:\\Program Files\\Java\\javafx-sdk-21.0.2\\lib\\' --add-modules javafx.controls,javafx.fxml,javafx.graphics,javafx.media"
        }
    ]
}